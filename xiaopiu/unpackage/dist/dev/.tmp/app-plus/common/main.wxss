}
.uni-page-body {
	width: 100%;
	-webkit-box-flex: 1;
	-webkit-flex-grow: 1;
	    -ms-flex-positive: 1;
	        flex-grow: 1;
	overflow-x: hidden;
}
.uni-padding-wrap{
	width:690rpx;
	padding:0 30rpx;
}
.uni-word {
	text-align: center;
	padding:200rpx 100rpx;
}
.uni-title {
	font-size:30rpx;
	font-weight:500;
	padding:20rpx 0;
	line-height:1.5;
}
.uni-text{
	font-size:28rpx;
}
.uni-title text{
	font-size:24rpx; 
	color:#888;
}
.uni-text-gray{
