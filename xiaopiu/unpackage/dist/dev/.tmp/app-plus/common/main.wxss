.b-b:after,
.b-t:after {
  position: absolute;
  z-index: 3;
  left: 0;
  right: 0;
  height: 0;
  content: '';
  -webkit-transform: scaleY(0.5);
      -ms-transform: scaleY(0.5);
          transform: scaleY(0.5);
  border-bottom: 1px solid #333;
}
.b-b:after {
  bottom: 0;
}
.b-t:after {
  top: 0;
}
/* button */
uni-button,
button {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  font-weight: normal;
}
uni-button.no-border:before, uni-button.no-border:after, button.no-border:before, button.no-border:after {
    border: 0;
}
uni-button[type=default],
button[type=default] {
  color: #303133;
}
/* input  */
.input-placeholder {
  color: #999999;
}
.placeholder {
  color: #999999;
}

